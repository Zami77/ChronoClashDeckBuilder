@model ChronoClashDeckBuilder.Models.ViewModels.DeckBuilderListViewModel

@{
    ViewData["Title"] = "Deck Builder";
}
<h1>Deck Builder!</h1>
<p>Still have to implement picking a card and adding to deck.</p>
<p>
</p>
<tbody>
    @foreach (var item in Model.NewDeck)
    {
        <tr>
            <td>
                @item.CardName
            </td>
        </tr>
    }
</tbody>
<tbody>
    @foreach (var item in Model.Cards)
    {
        <tr>
            <td>
                <input type="image" src="~/Images/Chrono Clash Card Images/@item.CardImage"
                       alt="@item.CardName"
                       style="width:136px;height:185px;" />
            </td>
        </tr>
    }
</tbody>
